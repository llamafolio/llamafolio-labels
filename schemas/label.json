{
  "id": "http://json-schema.org/draft-04/schema#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "labels": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "value": {
              "type": "string"
            }
          },
          "required": ["type", "value"]
        }
      ]
    },
    "links": {
      "type": "object",
      "properties": {
        "website": {
          "type": "string"
        },
        "twitter": {
          "type": "string"
        },
        "github": {
          "type": "string"
        },
        "telegram": {
          "type": "string"
        }
      }
    }
  },
  "required": ["labels", "links"]
}
